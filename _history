     drop(20)\n    }\n}\nfunction rotate (数値2: number) {\n    if (angle != 数値2) {\n        pins.servoWritePin(AnalogPin.P9, 数値2)\n        angle = 数値2\n        basic.pause(500)\n    }\n}\nlet lastWait = 0\nlet wait = 0\nlet angle = 0\nHX711.SetPIN_SCK(DigitalPin.P1)\nHX711.SetPIN_DOUT(DigitalPin.P0)\nHX711.begin()\nHX711.set_scale(3000)\nHX711.tare(10)\nlet 配列 = [0, 1]\nangle = -1\nrotate(90)\npins.servoWritePin(AnalogPin.P8, 90)\nbasic.forever(function () {\n    wait = HX711.get_units(5) * 4.05\n    if (wait < 5) {\n        let wireWait: number[] = []\n        wireWait.push(wait)\n    }\n    if (Math.abs(wait - lastWait) < 2) {\n        ceckWait()\n    }\n    lastWait = wait\n})\n","pxt.json":"{\n    \"name\": \"お手玉仕分け\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"pxt-HX711\": \"github:daferdur/pxt-myhx711#v1.0.18\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"blocksprj\"\n}\n"}},{"timestamp":1730176150461,"editorVersion":"7.0.51","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"sHCIwdP?^//7q8L2uij)\">wait</variable><variable id=\"7c34uhpKO(wiLQTzT/cD\">angle</variable><variable id=\"%uo%]8*~/4!=!F{/S*,u\">wireWait</variable><variable id=\"F9V,fWb%nR#L5BozF[4H\">lastWait</variable><variable id=\"Xh-ZX)d[IAsx58fq0wny\">配列</variable></variables><block type=\"function_definition\" collapsed=\"true\" x=\"433\" y=\"-212\"><mutation name=\"rotate\" functionid=\"m}%;rN[{O8cB1LjQAl22\"><arg name=\"数値2\" id=\"pg80y3800on8s97ar1sb1j\" type=\"number\"></arg></mutation><field name=\"function_name\">rotate</field><value name=\"pg80y3800on8s97ar1sb1j\"><block type=\"argument_reporter_number\"><mutation duplicateondrag=\"true\"></mutation><field name=\"VALUE\">数値2</field></block></value><statement name=\"STACK\">